<h1>Created Articles</h1>

<%= link_to 'Create New', new_article_path %>

<% @articles.each do |a| %>
    <ul>
        <li><%= a.title %> &nbsp &nbsp <%= link_to 'Edit', edit_article_path(a) %>  <%= link_to 'Show', article_path(a) %>  <%= link_to 'Delete', article_path(a), method: :delete, data: {confirm: "Are you sure?" } %></li>
    </ul>
<% end %>
